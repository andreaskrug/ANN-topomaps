{
  "model": "FMNIST-CNN_SHALLOW",
  "data": "FMNIST",
  "layers": [
    0, 2
  ],
  "distance_metric": "cosine",
  "nap_params": {
    "n_random_examples": 200,
    "error_mode": "binary_contrast"
  },
  "topomap_params": {
    "neuron_activations": {
      "use_naps": false,
      "n_random_examples": 1000,
      "balance_by_label": true,
      "use_correct_only": true
    },
    "methods": ["UMAP_PSO"]
  },
  "plot_params": {
    "plot_inputs": true,
    "avg_inputs": false,
    "ordered": true,
    "scale": 2.0,
    "use_title": true,
    "plot_individually": false
  }
}